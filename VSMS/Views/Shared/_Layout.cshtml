    <script type="text/javascript">
        $(document).on("click", "#open-Modal", function () {
            var id = $(this).data('id');
            $(".tt-form-default02 #IdCar").val(id);

            $("#submitDriveTest").click(function () {

                console.log(id);
                AddDriverTest(id);
            })

        });


        function AddDriverTest(idCar) {
            var IdMem = $("#IdMember").val();
                var today = new Date();
                var dd = String(today.getDate()).padStart(2, '0');
                var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
                var yyyy = today.getFullYear();

            today = yyyy + '-' + mm + '-' + dd;
            //var dateTest = new Date(today);
            var res = Validate();
            if (res == false) {
                return false;
            };
                var drTestOjb = {
                    IdMember: IdMem,
                    IdCar: idCar,
                    Note: $('#dateTest').val(),
                    Status: 0,// trạng thái chưa accept drive test
                    CreatedAt: today

                };
                console.log("submitDriveTest");
                console.log(drTestOjb);
                $.ajax({
                    url: '@Url.Action("CreateDriverTest", "Inventory")',
                    type: "POST",
                    data: drTestOjb,
                    success: function (rs) {
                        if (!rs) {
                            toastr.error('Register Drive Test False!', 'Thông báo', { timeOut: 2000 });
                        } else {
                            toastr.success('Register Success!', 'Thông báo', { timeOut: 2000 });
                        }
                    },
                    error: function (e) {
                        console.log(e.responseText);
                    }
                });
    }
    // validate data
    function Validate() {
        var isValid = true;
        var valueDate = document.getElementById('dateTest').value;
        if (!valueDate) {
            toastr.error('Choose Date', 'Thông báo', { timeOut: 2000 });
            isValid = false;
        }
        return isValid;
    }
    </script>
